{
  "name": "decorating",
  "version": "1.0.3",
  "description": "Helpers for decorating classes and properties",
  "repository": "farwayer/decorating",
  "author": "farwayer <farwayer@gmail.com>",
  "license": "WTFPL",
  "keywords": [
    "decorate",
    "decorator",
    "decorating"
  ],
  "main": "lib",
  "jsnext:main": "esm",
  "module": "esm",
  "react-native": "esm",
  "scripts": {
    "build": "yarn clean && yarn build-lib && yarn build-esm",
    "build-lib": "babel src --out-dir lib",
    "build-esm": "ESM=1 babel src --out-dir esm",
    "clean": "rm -rf lib esm",
    "test": "mocha",
    "cover": "istanbul cover _mocha --report lcovonly -- --reporter spec && cat coverage/lcov.info | coveralls && rm -rf coverage",
    "prepack": "yarn build && yarn test"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "coveralls": "^3.0.2",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^5.2.0",
    "should": "^13.2.3"
  },
  "dependencies": {}
}
